#Operaciones Basicas 
n1=float(input("Ingrese el primer numero:"))
n2=float (input("Ingrese el segundo numero:"))

suma=n1+n2
resta=n1-n2 
multi=n1*n2 

print("La suma es:", suma)
print("La resta es:", resta)
print("La multiplicacion es:", multi)


#Imprimir numero pares de 1 a 101
print("\n")
for i in range (2,20,2): 
    print(i)

#Imprimir numeros y señalar cuales son pares. 
print("\n")
for i in range (2,20): 
    if (i%2==0):
        print(i,"Es par")
    else:
        print(i,"Es impart")
#Imprimir serie de Fibonacci 
print ("\n")
def fibonacci(n): 
    secuencia=[0,1]
    while len(secuencia)<n: 
        siguiente_valor = secuencia[-1] + secuencia[-2]  # Sumar los dos últimos valores de la secuencia
        secuencia.append(siguiente_valor)
    return secuencia

print(fibonacci(10))
